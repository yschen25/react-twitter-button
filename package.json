{
  "name": "react-twitter-button",
  "version": "1.0.9",
  "description": "React button for twitter, you can choose different kinds of icons and custom buttons.",
  "main": "dist/react-twitter-button.js",
  "scripts": {
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack --mode=development",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack --mode=production",
    "build": "npm run build:umd && npm run build:umd:min",
    "lint": "eslint src/*.js",
    "lint-fix": "eslint --fix src/*.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yschen25/React_Twitter_Button.git"
  },
  "keywords": [
    "react",
    "twitter",
    "button",
    "react-twitter-button"
  ],
  "author": "Yi-Shiuan Chen, yschen25@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yschen25/React_Twitter_Button/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "homepage": "https://github.com/yschen25/React_Twitter_Button#readme",
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.7.4",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.4",
    "autoprefixer-loader": "^3.2.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "browserslist": "^4.9.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.3.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^9.0.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.0",
    "react-hot-loader": "^4.12.18",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "babel-eslint": "^10.0.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.1.1",
    "url-loader": "^3.0.0",
    "user": "0.0.0",
    "webpack-cli": "^3.3.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
